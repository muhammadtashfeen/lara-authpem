{
    "name": "muhammadtashfeen/lara-authpem",
    "description": "A package that makes settings permissions and authorizations easier in Laravel.",
    "type": "library",
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.5",
        "laravel/pint": "dev-main"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MuhammadTashfeen\\LaraAuthpem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MuhammadTashfeen\\LaraAuthpem\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Muhammad Tashfeen"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": false
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MuhammadTashfeen\\LaraAuthpem\\AuthServiceProvider"
            ]
        }
    }
}
